public static void main(String[] args) {
	// TODO Auto-generated method stub
	Scanner sc = new Scanner(System.in);
	int m, n;
	m = sc.nextInt(); n = sc.nextInt();
	
	int[][] mat = new int[m+2][n+2];
	int[][] result = new int[m][n];
	
	
	for(int i = 1; i <= m; i++){
		String str = sc.next();
		for(int j=1; j <= n; j++){
			if(str.charAt(j-1) == '*')
				mat[i][j] = 1;
			else
				mat[i][j] = 0;
		}
	}//배열에 넣었음. 지뢰있으면 1, 지뢰없으면 0.
	
	for(int i = 1; i <= m; i++){
		for(int j = 1; j <= n; j++){
			int count = 0;//지뢰개수
			
			for(int k=i-1; k <= i+1; k++)
				for(int l=j-1; l <= j+1; l++){
					if(k==i && l==j) continue;
					if(mat[k][l] == 1)
						count++;
				}
			result[i-1][j-1] = count;
		}
	}
	for(int i = 0; i < m; i++){
		for(int j = 0; j < n; j++){
			if( mat[i+1][j+1] == 1)
				System.out.print("*");
			else System.out.print(result[i][j]);
		}
		System.out.println("");
	}
}
}
	